<% if @ownedcourses.size != 0 %>     
    <% @ownedcourses.each do |ownedcourse|%>
        <% owned_course_name = Course.find(id = ownedcourse.course_id)%>
        <% content = Content.find(course_id = ownedcourse.course_id) %>
        <p><%= ownedcourse.course_id %> </p>
        <p><%= link_to "#{owned_course_name.name}", content_path(content) %> </p>
    <% end %>
<% else %>
    <p>You dont have</p>
<% end %>